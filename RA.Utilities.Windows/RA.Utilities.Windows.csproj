<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
		<Version>0.0.1</Version>
		<Authors>Regis Antunes</Authors>
		<Description>Utilities package to run on Windows only</Description>
		<PackageReleaseNotes>Windows focused features from RegisAntuens.Utilities was migrated to this package</PackageReleaseNotes>
		<PackageId>RegisAntunes.Utilities.Windows</PackageId>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>
	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="\"/>
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="RegisAntunes.Utilities" Version="0.2.1" />
	</ItemGroup>
</Project>
